{
  "name": "SVG Exporter",
  "version": "2023.07.13",
  "manifest_version": 3,
  "description": "A tool for downloading SVG elements by CTRL+ left-clicking them and converting them to .png as well. App logo created using DALL-E.",
  "icons": {
    "16": "svg_downloader_logo_16.png",
    "32": "svg_downloader_logo_32.png",
    "48": "svg_downloader_logo_48.png",
    "128": "svg_downloader_logo_128.png"
  },
  "permissions": ["scripting", "activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["add-svg-buttons.js"]
    }
  ]
}
